package de.ww.openweather.utils;

/**
 * DTO f&uuml;r das HTML-Tile der Wettervorhersage
 * @author Wolfram
 *
 */
public class WettervorhersageTileDTO {

	private String datumString;
	private double tempMin;
	private double tempMax;
	private String beschreibung;
	private String iconUrl;
	
	public WettervorhersageTileDTO() {
		
	}

	public String getDatumString() {
		return datumString;
	}

	public void setDatumString(String datumString) {
		this.datumString = datumString;
	}

	public double getTempMin() {
		return tempMin;
	}

	public void setTempMin(double tempMin) {
		this.tempMin = tempMin;
	}

	public double getTempMax() {
		return tempMax;
	}

	public void setTempMax(double tempMax) {
		this.tempMax = tempMax;
	}
	
	public String getBeschreibung() {
		return beschreibung;
	}

	public void setBeschreibung(String beschreibung) {
		this.beschreibung = beschreibung;
	}

	public String getIconUrl() {
		return iconUrl;
	}

	public void setIconUrl(String iconUrl) {
		this.iconUrl = iconUrl;
	}

	@Override
	public String toString() {
		return "WettervorhersageTileDTO [\ndatumString=" + datumString + "\n, tempMin=" 
				+ tempMin + ", tempMax=" + tempMax
				+ "\n, beschr.=" + beschreibung
				+ "\n, iconUrl=" + iconUrl
				+ "]\n";
	}
	
}
